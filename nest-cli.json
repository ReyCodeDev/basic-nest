{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [{
      "include": "./views/**/*",
      "exclude": "./views/.gitkeep",
      "outDir": "dist/",
      "watchAssets": true
    },{
      "include": "./public/**/*",
      "exclude": "./public/.gitkeep",
      "outDir": "dist/",
      "watchAssets": true
    }
    ]
  }
}
